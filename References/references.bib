@article{Pradhan.2012, 
year = {2012}, 
title = {{A model for the evolution of developmental arrest in male orangutans}}, 
author = {Pradhan, Gauri R. and Noordwijk, Maria A. van and Schaik, Carel van}, 
journal = {American Journal of Physical Anthropology}, 
issn = {0002-9483}, 
doi = {10.1002/ajpa.22079}, 
pmid = {22552966}, 
abstract = {{Male Sumatran orangutans (Pongo abelii) may delay for many years the acquisition of the full array of secondary sexual traits, including their characteristic cheek flanges. Such flexible developmental arrest is unique among male primates. Among male Bornean orangutans (Pongo pygmaeus) such long delays appear less common. Here, we develop a simple model to identify the conditions under which developmental arrest can be adaptive. We show that the baseline strategy (i.e., males are not susceptible to arrest) cannot be invaded by the flexible strategy (i.e., males can arrest their development when the conditions are unfavorable) when the potential for high‐ranking unflanged or flanged males to monopolize sexual access to females is low. In contrast, at high monopolization potential, the flexible strategy is the evolutionarily stable strategy. We also derive the proportion of flanged males in the population for each combination of monopolization values. This model concurs with field data that found a different monopolization potential between Bornean and Sumatran flanged males and a lower proportion of flanged males in the population in Sumatran orangutans. Pronounced developmental arrest is linked to very low adult mortality, which explains why it is so limited in its taxonomic distribution. Am J Phys Anthropol 2012. © 2012 Wiley Periodicals, Inc.}}, 
pages = {18--25}, 
number = {1}, 
volume = {149}, 
local-url = {file://localhost/Users/noah/papers_library/Pradhan_2012_2.pdf}
}
@article{Poorter.2021, 
year = {2021}, 
title = {{Multidimensional tropical forest recovery}}, 
author = {Poorter, Lourens and Craven, Dylan and Jakovac, Catarina C. and Sande, Masha T. van der and Amissah, Lucy and Bongers, Frans and Chazdon, Robin L. and Farrior, Caroline E. and Kambach, Stephan and Meave, Jorge A. and Muñoz, Rodrigo and Norden, Natalia and Rüger, Nadja and Breugel, Michiel van and Zambrano, Angélica María Almeyda and Amani, Bienvenu and Andrade, José Luis and Brancalion, Pedro H. S. and Broadbent, Eben N. and Foresta, Hubert de and Dent, Daisy H. and Derroire, Géraldine and DeWalt, Saara J. and Dupuy, Juan M. and Durán, Sandra M. and Fantini, Alfredo C. and Finegan, Bryan and Hernández-Jaramillo, Alma and Hernández-Stefanoni, José Luis and Hietz, Peter and Junqueira, André B. and N’dja, Justin Kassi and Letcher, Susan G. and Lohbeck, Madelon and López-Camacho, René and Martínez-Ramos, Miguel and Melo, Felipe P. L. and Mora, Francisco and Müller, Sandra C. and N’Guessan, Anny E. and Oberleitner, Florian and Ortiz-Malavassi, Edgar and Pérez-García, Eduardo A. and Pinho, Bruno X. and Piotto, Daniel and Powers, Jennifer S. and Rodríguez-Buriticá, Susana and Rozendaal, Danaë M. A. and Ruíz, Jorge and Tabarelli, Marcelo and Teixeira, Heitor Mancini and Sampaio, Everardo Valadares de Sá Barretto and Wal, Hans van der and Villa, Pedro M. and Fernandes, Geraldo W. and Santos, Braulio A. and Aguilar-Cano, José and Almeida-Cortez, Jarcilene S. de and Alvarez-Davila, Esteban and Arreola-Villa, Felipe and Balvanera, Patricia and Becknell, Justin M. and Cabral, George A. L. and Castellanos-Castro, Carolina and Jong, Ben H. J. de and Nieto, Jhon Edison and Espírito-Santo, Mário M. and Fandino, Maria C. and García, Hernando and García-Villalobos, Daniel and Hall, Jefferson S. and Idárraga, Alvaro and Jiménez-Montoya, Jaider and Kennard, Deborah and Marín-Spiotta, Erika and Mesquita, Rita and Nunes, Yule R. F. and Ochoa-Gaona, Susana and Peña-Claros, Marielos and Pérez-Cárdenas, Nathalia and Rodríguez-Velázquez, Jorge and Villanueva, Lucía Sanaphre and Schwartz, Naomi B. and Steininger, Marc K. and Veloso, Maria D. M. and Vester, Henricus F. M. and Vieira, Ima C. G. and Williamson, G. Bruce and Zanini, Kátia and Hérault, Bruno}, 
journal = {Science}, 
issn = {0036-8075}, 
doi = {10.1126/science.abh3629}, 
pmid = {34882461}, 
abstract = {{Tropical forests disappear rapidly because of deforestation, yet they have the potential to regrow naturally on abandoned lands. We analyze how 12 forest attributes recover during secondary succession and how their recovery is interrelated using 77 sites across the tropics. Tropical forests are highly resilient to low-intensity land use; after 20 years, forest attributes attain 78\% (33 to 100\%) of their old-growth values. Recovery to 90\% of old-growth values is fastest for soil (<1 decade) and plant functioning (<2.5 decades), intermediate for structure and species diversity (2.5 to 6 decades), and slowest for biomass and species composition (>12 decades). Network analysis shows three independent clusters of attribute recovery, related to structure, species diversity, and species composition. Secondary forests should be embraced as a low-cost, natural solution for ecosystem restoration, climate change mitigation, and biodiversity conservation.}}, 
pages = {1370--1376}, 
number = {6573}, 
volume = {374}, 
local-url = {file://localhost/Users/noah/papers_library/Poorter_2021_3.pdf}
}
